            </jaxb:schemaBindings>
        </jaxb:bindings>
    </jaxb:bindings>

    <jaxb:bindings schemaLocation="../resources/schema/samlv2/xmldsig-core-schema.xsd">
        <jaxb:bindings node="/xs:schema">
            <jaxb:schemaBindings>
                <jaxb:package name="org.jboss.seam.security.external.jaxb.xmldsig"/>
            </jaxb:schemaBindings>
        </jaxb:bindings>
    </jaxb:bindings>

</jaxb:bindings>
